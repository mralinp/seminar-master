	doi = {10.1145/3219819.3220042},
	isbn = {9781450355520},
	keywords = {dimension reduction, high-dimensional data, outlier detection, anomaly detection, prior knowledge, representation learning, ultrahigh-dimensional data},
	location = {London, United Kingdom},
	numpages = {10},
	pages = {2041--2050},
